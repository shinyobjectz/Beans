{
    "mcpServers": {
      "valyu": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@valyu/mcp-server"],
        "env": {
          "VALYU_API_KEY": "${VALYU_API_KEY}"
        }
      },
      "memory": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-memory"]
      },
      "sequential-thinking": {
        "type": "stdio",
        "command": "npx",
        "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
      }
    }
  }
  